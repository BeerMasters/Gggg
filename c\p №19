#include <iostream>
using namespace std;

int main() {
	setlocale(0, "");

	int arrINT[10]{
	1, 20, 32, 4, 1, 2, 2, 54, 23, 0
	};
	int arrSHORT[10]{
	1, 20, 32, 4, 1, 2, 2, 54, 23, 0
	};
	int arrLONG[10]{
	1, 20, 32, 4, 1, 2, 2, 54, 23, 0
	};
	int arrFLOAT[10]{
	1.1, 20.2, 32, 4.5, 1, 2, 2.2, 54, 23, 0
	};
	int arrDOUBLE[10]{
	1, 20.22, 32, 4, 1, 2.14, 2, 54, 23, 0
	};
	int arrCHAR[10]{
		'a','b','c','d','f','g','h','i','k','p'
	};
	int arrBOOL[10]{
		true, false, true, false, true, false, true, false, true, false
	};
	string arrSTRING[10] = {
	"Иванов", "Петров", "Сидоров",
	"Ахмедов", "Ерошкин", "Выхин",
	"Андеев", "Вин Дизель", "Картошкин", "Чубайс"
	};
	cout << "[+] Массив arrINT" << endl;
	for (int i = 0; i < 10; i++)
	{
		cout << "[+] arrINT[" << i << "] | значение: " << arrINT[i] << endl;
	};
	cout << "[+] Массив arrSHORT" << endl;
	for (int i = 0; i < 10; i++)
	{
		cout << "[+] arrSHORT[" << i << "] | значение: " << arrSHORT[i] << endl;
	};
	cout << "[+] Массив arrLONG" << endl;
	for (int i = 0; i < 10; i++)
	{
		cout << "[+] arrLONG[" << i << "] | значение: " << arrLONG[i] << endl;
	};
	cout << "[+] Массив arrFLOAT" << endl;
	for (int i = 0; i < 10; i++)
	{
		cout << "[+] arrFLOAT[" << i << "] | значение: " << arrFLOAT[i] << endl;
	};
	cout << "[+] Массив arrDOUBLE" << endl;
	for (int i = 0; i < 10; i++)
	{
		cout << "[+] arrDOUBLE[" << i << "] | значение: " << arrDOUBLE[i] << endl;
	};
	cout << "[+] Массив arrCHAR" << endl;
	for (int i = 0; i < 10; i++)
	{
		cout << "[+] arrCHAR[" << i << "] | значение: " << arrCHAR[i] << endl;
	};
	cout << "[+] Массив arrBOOL" << endl;
	for (int i = 0; i < 10; i++)
	{
		cout << "[+] arrBOOL[" << i << "] | значение: " << arrBOOL[i] << endl;
	};

	return 0;
}

