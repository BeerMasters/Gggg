#include <iostream>
using namespace std;

int main() {
	setlocale(0, "");


	cout << "[+] Переводчик" << "\n" << "\n"
		<< "[1] Русские слова" << "\n"
		<< "[2] Английские слова" << "\n"
		<< "[3] Выйти" << endl;

	int a;
	cin >> a;
	int b;

	switch (a)
	{
	case 1:
		cout
			<< "[+] Переводчик 'Списка русских слов'" << "\n"
			<< "[1] Желание" << "\n"
			<< "[2] Ржавый" << "\n"
			<< "[3] Семнадцать" << "\n"
			<< "[4] Рассвет" << "\n"
			<< "[5] Печь" << "\n"
			<< "[6] Девять" << "\n"
			<< "[7] Добросердечный" << "\n"
			<< "[8] Возвращение" << "\n"
			<< "[9] Родина" << "\n"
			<< "[10] Один" << "\n"
			<< "[11] Грузовой" << "\n"
			<< "[12] Вагон" << "\n"
			<< "[13] Рука" << "\n"
			<< "[14] Брат" << "\n"
			<< "[15] Потеря" << endl;
		cin >> b;
		switch (b)
		{
		case 1:
			cout << "Перевод: Желание --> wish" << endl;
			break;
		case 2:
			cout << "Перевод: Ржавый --> Rusty" << endl;
			break;
		case 3:
			cout << "Перевод: Семнадцать --> Seventeen" << endl;
			break;
		case 4:
			cout << "Перевод: Рассвет --> Dawn" << endl;
			break;
		case 5:
			cout << "Перевод: Печь --> Bake" << endl;
			break;
		case 6:
			cout << "Перевод: Девять --> Nine" << endl;
			break;
		case 7:
			cout << "Перевод: Добросердечный --> kindhearted" << endl;
			break;
		case 8:
			cout << "Перевод: Возвращение --> Return" << endl;
			break;
		case 9:
			cout << "Перевод: Родина --> Motherland" << endl;
			break;
		case 10:
			cout << "Перевод: Один --> One" << endl;
			break;
		case 11:
			cout << "Перевод: Грузовой --> Cargo" << endl;
			break;
		case 12:
			cout << "Перевод: Вагон --> carriage" << endl;
			break;
		case 13:
			cout << "Перевод: Рука --> Hand" << endl;
			break;
		case 14:
			cout << "Перевод: Брат --> Brother" << endl;
			break;
		case 15:
			cout << "Перевод: Потеря --> loss" << endl;
			break;

		default:
			cout << "Выбора под таким номером " << endl;
			break;
		}
		break;
	case 2:
		cout
			<< "[+] Переводчик 'Списка английских слов'" << "\n" << endl
			<< "[1] wish" << "\n"
			<< "[2] Rusty" << "\n"
			<< "[3] Seventeen" << "\n"
			<< "[4] Dawn" << "\n"
			<< "[5] Bake" << "\n"
			<< "[6] Nine" << "\n"
			<< "[7] kindhearted" << "\n"
			<< "[8] Return" << "\n"
			<< "[9] Motherland" << "\n"
			<< "[10] One" << "\n"
			<< "[11] Cargo" << "\n"
			<< "[12] carriage" << "\n"
			<< "[13] Hand" << "\n"
			<< "[14] Brother" << "\n"
			<< "[15] loss" << endl;
		cin >> b;
		switch (b)
		{
			{
		case 1:
			cout << "Перевод: wish --> Желание" << endl;
			break;
		case 2:
			cout << "Перевод: Rusty --> Ржавый" << endl;
			break;
		case 3:
			cout << "Перевод: Seventeen --> Семнадцать" << endl;
			break;
		case 4:
			cout << "Перевод: Dawn --> Рассвет" << endl;
			break;
		case 5:
			cout << "Перевод: Bake --> Печь" << endl;
			break;
		case 6:
			cout << "Перевод: Nine --> Девять" << endl;
			break;
		case 7:
			cout << "Перевод: kindhearted --> Добросердечный" << endl;
			break;
		case 8:
			cout << "Перевод: Return --> Возвращение" << endl;
			break;
		case 9:
			cout << "Перевод: Motherland --> Родина" << endl;
			break;
		case 10:
			cout << "Перевод: One --> Один" << endl;
			break;
		case 11:
			cout << "Перевод: Cargo --> Грузовой" << endl;
			break;
		case 12:
			cout << "Перевод: carriage --> Вагон" << endl;
			break;
		case 13:
			cout << "Перевод: Hand --> Рука" << endl;
			break;
		case 14:
			cout << "Перевод: Brother --> Брат" << endl;
			break;
		case 15:
			cout << "Перевод: loss --> Потеря" << endl;
			break;

		default:
			cout << "Выбора под таким номером " << endl;
			break;
			}
			break;
		}
	case 3:
		exit;
		
		break;
	default:
		cout << "Выбора под таким номером " << endl;
		break;
	}



	return 0;
}
